<UserControl
    x:Class="MichelMichels.PSP.PBP.Gui.Views.DefaultSubFileView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:MichelMichels.PSP.PBP.Gui.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewmodels="clr-namespace:MichelMichels.PSP.PBP.Gui.ViewModels"
    d:DataContext="{d:DesignInstance viewmodels:DefaultSubFileViewModel}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <TextBlock
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            FontWeight="16"
            Foreground="Gray"
            Text="{Binding Name}" />
    </Grid>
</UserControl>
